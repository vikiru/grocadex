{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "target": "ES6",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
